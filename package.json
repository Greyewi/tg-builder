{
  "name": "tg-builder",
  "version": "0.1.6",
  "description": "Builder library for the organisation of a lot of telegram bots.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist/**/*"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Greyewi/tg-builder.git"
  },
  "scripts": {
    "test": "jest",
    "build": "tsc example/tg-rpg/index.ts",
    "format": "prettier --write .",
    "example": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\"",
    "build:npm": "rm -rf ./dist && tsc --build tsconfig.json && npm publish",
    "prebuild:npm": "rm -rf ./dist && tsc --build tsconfig.json && npm publish --dry-run"
  },
  "keywords": [
    "telegram",
    "telegraf",
    "tg-builder",
    "telegram-builder",
    "telegram-bot",
    "bot",
    "tg-design"
  ],
  "author": "Suntsev Sergey",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.3.1",
    "lowdb": "^6.0.1",
    "telegraf": "^4.12.3-canary.1",
    "tg-builder": "^0.1.3"
  },
  "devDependencies": {
    "@telegraf/types": "^6.7.0",
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@types/jest": "^29.5.2",
    "@types/lowdb": "^1.0.11",
    "@types/node": "^16.7.13",
    "babel-jest": "^29.5.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3"
  }
}
